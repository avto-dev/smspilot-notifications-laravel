{
    "name": "avto-dev/smspilot-notifications-laravel",
    "description": "Notifications channel for SMS Pilot service",
    "keywords": [
        "laravel",
        "sms-pilot",
        "notification"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avto-dev",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "guzzlehttp/guzzle": "^6.0",
        "laravel/framework": ">=5.4.0 <5.7.0"
    },
    "require-dev": {
        "laravel/laravel": ">=5.4.0 <5.7.0",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "symfony/var-dumper": "~3.2 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\SmsPilotNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\SmsPilotNotifications\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\SmsPilotNotifications\\SmsPilotServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/smspilot-notifications-laravel/issues",
        "source": "https://github.com/avto-dev/smspilot-notifications-laravel"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
